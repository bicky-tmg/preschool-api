        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.3",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/cors": "^2.8.19",
